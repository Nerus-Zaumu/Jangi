<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" styleClass="background-style" stylesheets="@styles/styles.css" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="jangi.user.jangiuser.ContributeController">
   <children>
      <Label layoutX="169.0" layoutY="47.0" text="Make Contribution">
         <font>
            <Font size="25.0" />
         </font>
      </Label>
      <Label layoutX="130.0" layoutY="120.0" text="Type">
         <font>
            <Font size="25.0" />
         </font>
      </Label>
      <Label layoutX="88.0" layoutY="185.0" text="Amount">
         <font>
            <Font size="25.0" />
         </font>
      </Label>
      <ComboBox fx:id="type" layoutX="250.0" layoutY="116.0" prefHeight="39.0" prefWidth="199.0" promptText="\@type" />
      <ComboBox fx:id="amount" layoutX="250.0" layoutY="185.0" prefHeight="39.0" prefWidth="199.0" promptText="\@amount" />
      <Button layoutX="257.0" layoutY="270.0" mnemonicParsing="false" onAction="#insertContribution" prefHeight="39.0" prefWidth="87.0" text="Contribute" />
      <Button layoutX="249.0" layoutY="335.0" mnemonicParsing="false" onAction="#switchToDashboard" prefHeight="39.0" prefWidth="102.0" text="Back Home" />
   </children>
</AnchorPane>
