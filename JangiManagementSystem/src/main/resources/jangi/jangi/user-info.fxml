          <TableColumn fx:id="UNAME" prefWidth="221.0" text="Name" />
            <TableColumn fx:id="USAVINGS" prefWidth="163.0" text="Savings" />
            <TableColumn fx:id="ULOANS" prefWidth="140.0" text="Loans" />
        </columns>
      </TableView>
      <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#switchToDashboard" prefHeight="31.0" prefWidth="83.0" text="Back" />
