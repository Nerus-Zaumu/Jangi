<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" styleClass="background-style" stylesheets="@styles/styles.css" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="jangi.jangi.AuxController">
   <children>
      <Label layoutX="160.0" layoutY="53.0" text="Configure Interest Rates">
         <font>
            <Font size="23.0" />
         </font>
      </Label>
      <Label layoutX="81.0" layoutY="134.0" text="Loan range">
         <font>
            <Font size="23.0" />
         </font>
      </Label>
      <Label layoutX="75.0" layoutY="206.0" text="Interest Rate">
         <font>
            <Font size="23.0" />
         </font>
      </Label>
      <Button layoutX="248.0" layoutY="289.0" mnemonicParsing="false" onAction="#insertInterestRate" prefHeight="36.0" prefWidth="105.0" text="Set Rate" />
      <Button fx:id="interestBtn" layoutX="248.0" layoutY="343.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="36.0" prefWidth="105.0" text="Go Back" />
      <ComboBox fx:id="loanRange" layoutX="263.0" layoutY="137.0" prefHeight="24.0" prefWidth="163.0" promptText="\@range" />
      <ComboBox fx:id="interestRate" layoutX="263.0" layoutY="209.0" prefHeight="24.0" prefWidth="163.0" promptText="\@rate" />
   </children>
</AnchorPane>
